{  "description": "Adds ability to manage shipments with TNT easier.",  "manifest_version": 2,  "name": "myTNT toolbar",  "version": "1.1",  "homepage_url": "https://my.tnt.com",  "icons":     {      "32": "icons/tntLogo-mobile-32.png"    },  "applications":     {      "gecko":       {        "id": "cit.sk@tnt.com",        "strict_min_version": "45.0"      }    },  "content_scripts":     [      {        "matches": ["<all_urls>"],        "js": ["scripts/content.js"]      }    ],   "background":     {      "scripts": ["scripts/background.js"]             },      "browser_action":     {      "browser_style": false,      "default_icon": "icons/tntLogo-mobile-32.png",      "default_title": "myTNT",      "default_popup": "popup/popup.html"    },  "permissions":     [      "tabs",      "cookies",      "storage",      "webNavigation",      "http://www.tnt.com/*",      "https://my.tnt.com/*"    ],  "options_ui":     {      "page": "options/options.html"    }     }